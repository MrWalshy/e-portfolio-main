    <%- include("./partials/header") %>
    <div class="container pt-5 mt-5">
        <form action="/contact" method="POST">
            <div class="form-group">
                <label for="userEmail">Email</label>
                <input class="form-control" type="email" name="userEmail" id="emailInput" placeholder="Email" required>
            </div>
            <div class="form-group">
                <label for="subject">Subject</label>
                <input class="form-control" type="text" name="subject" id="subjectInput" required>
            </div>
            <div class="form-group">
                <label for="message">Message</label>
                <textarea class="form-control" name="message" id="emailMessage" cols="30" rows="10"></textarea>
            </div>
            <div class="form-group">
                <button class="form-control btn btn-primary" type="submit">Submit</button>
            </div>
        </form>
    </div>
    <%- include("./partials/footer") %>